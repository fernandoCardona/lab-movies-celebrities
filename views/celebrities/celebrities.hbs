<h1>Celebrities List</h1>
<a href="/" class="go-back">Go back</a>
<ul class="block-celebrities">
  {{#each celebrities}}
      <li>
        <p>Name: {{this.name}}</p>
        {{!-- <p>Occupation: {{this.occupation}}</p>
        <p>CatchPhrase: {{this.catchPhrase}}</p> --}}
        <a href="/celebrities/{{this.id}}">See more</a>
        <form action="/celebrities/{{this._id}}/delete" method="POST">
            <button>Delete</button>
        </form>
      </li>
  {{/each}}
</ul>
